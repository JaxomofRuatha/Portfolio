-var socialLinks = viewData.socialLinks

nav.socialbot
    a(class="socialbot__link socialbot__link--email" id=socialLinks.email.id href=socialLinks.email.href) 
        i(class=socialLinks.email.icon)
    div(class="socialbot__flex")
      each link, i in socialLinks.flex
        a(class="socialbot__link" href=link.href)
            i(class=link.icon)
      span(class="socialbot__desc")